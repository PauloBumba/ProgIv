  "components": {
    "schemas": {
      "AdressDto": {
        "required": [
          "city",
          "country",
          "district",
          "federalstate",
          "numberHouse",
          "zipCode"
        ],
        "type": "object",
        "properties": {
          "country": {
            "minLength": 1,
            "type": "string"
          },
          "zipCode": {
            "maxLength": 8,
            "minLength": 1,
            "type": "string"
          },
          "federalstate": {
            "minLength": 1,
            "type": "string"
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "district": {
            "minLength": 1,
            "type": "string"
          },
          "numberHouse": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateMedicationDto": {
        "required": [
          "name",
          "strength"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 2,
            "type": "string"
          },
          "strength": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "notes": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateScheduleDto": {
        "required": [
          "medicationId",
          "timeOfDay"
        ],
        "type": "object",
        "properties": {
          "medicationId": {
            "type": "string",
            "format": "uuid"
          },
          "timeOfDay": {
            "type": "string",
            "format": "date-span"
          },
          "enabled": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "DeleteUserDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ForgotPasswordDto": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "required": [
          "email",
          "passWord",
          "rememberMe"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "passWord": {
            "minLength": 1,
            "type": "string"
          },
          "rememberMe": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "RecoverPassswordDto": {
        "required": [
          "email",
          "password",
          "token"
        ],
        "type": "object",
        "properties": {
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "token": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "RegisterDto": {
        "required": [
          "addresses",
          "confirmPassword",
          "cpf",
          "dateOfBirth",
          "email",
          "fullName",
          "password",
          "phoneNumber",
          "role",
          "sex"
        ],
        "type": "object",
        "properties": {
          "fullName": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string",
            "format": "password"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string"
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string"
          },
          "sex": {
            "$ref": "#/components/schemas/Sex"
          },
          "addresses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AdressDto"
            }
          },
          "cpf": {
            "minLength": 1,
            "pattern": "^\\d{11}$",
            "type": "string"
          },
          "role": {
            "$ref": "#/components/schemas/TypeUser"
          }
        },
        "additionalProperties": false
      },
      "Sex": {
        "enum": [
          0,
          1
        ],
        "type": "integer",
        "format": "int32"
      },
      "StringEnvelopResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "nullable": true
          },
          "isSuccess": {
            "type": "boolean"
          },
          "data": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TypeUser": {
        "enum": [
          0,
          1
        ],
        "type": "integer",
        "format": "int32"
      },
      "UpdateMedicationDto": {
        "required": [
          "name",
          "strength"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 2,
            "type": "string"
          },
          "strength": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "notes": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateUserDto": {
        "required": [
          "userId"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "minLength": 1,
            "type": "string"
          },
          "fullName": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "sex": {
            "$ref": "#/components/schemas/Sex"
          },
          "role": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ValidateCodeDto": {
        "required": [
          "code",
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "code": {
            "maxLength": 6,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Insira 'Bearer' seguido do token JWT",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}